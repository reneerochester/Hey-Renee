<!-- Consider adding an manifest.appcache: h5bp.com/d/Offline -->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
  <meta charset="utf-8">

  <!-- Use the .htaccess and remove these lines to avoid edge case issues.
       More info: h5bp.com/b/378 -->
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <title>Hey Renee</title>
  <meta name="description" content=" Toronto Web developer jquery rails ajax">
  <meta name="author" content="Renee Rochester- reneerochester@gmail.com">

  <!-- Mobile viewport optimized: j.mp/bplateviewport -->
  <meta name="viewport" content="width=device-width,initial-scale=1">
  <script src="/javascripts/modernizr-2.0.6.min.js"></script>
   <%= stylesheet_link_tag "application", "jquery.slides" %>

   <link rel="icon" href="favicon.ico" type="image/x-icon"> 
   <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
  
  <%= csrf_meta_tag %>
  
</head>

<body class="namespace-<%= controller.controller_name %> namespace-action-<%=controller.action_name
%> ">

  <div id="container">
    <header class="website">
      <a href="/pages/home" id="logo">
        Hey Renee, Renee Rochester
      </a>
      <div class="social-media">
        <ul>
          <li><%= link_to "LinkedIn", "http://www.linkedin.com/pub/renee-rochester/41/ba4/32b", :target => 'blank', :class => 'icon-linkedin' %></li>
          <li><%= link_to "Twitter", "https://twitter.com/#!/heyrrochester", :target => 'blank', :class => 'icon-twitter' %></li>
          <li><%= link_to "Google+", "https://plus.google.com/113082254343556108536/posts", :target => 'blank', :class => 'icon-googleplus' %></li>
        </ul>
        
      </div>
    </header>
    <div id="main" role="main">

      <%if content_for? :aside_main %>
      <aside class="main">
        <div class="inner-aside-main">
          <%= yield :aside_main %>
        </div>
      </aside>
         <%end%>
         
      <section class="main">
        <div class="inner-section-main">
          <%= yield %>
        </div>
      </section>
      </div>

      <% if content_for? :after_main %>
        <div class="contact-icon"></div>
      <% end %>
     
  </div>
  <!--end of #container-->
  
   <% if content_for? :after_main%>
    <div id="after_main">
      <div class="inner-after-main">
        <%= yield :after_main %>
      </div>
    </div>
    <%end%>
    
</div> <!--! end of #container -->


  <!-- JavaScript at the bottom for fast page loading -->
  <!-- Grab Google CDN's jQuery, with a protocol relative URL; fall back to local if offline -->


  <%= javascript_include_tag %w(jquery jquery_ujs) %>
  <%= javascript_include_tag 'jquery-ui-1.8.16.min' %>
  <%= javascript_include_tag 'jquery.slides','application'%>

  <script type="text/javascript">

    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', 'UA-28931624-1']);
    _gaq.push(['_trackPageview']);

    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

  </script>

  <!-- Prompt IE 6 users to install Chrome Frame. Remove this if you want to support IE 6.
       chromium.org/developers/how-tos/chrome-frame-getting-started -->
  <!--[if lt IE 7 ]>
    <script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.3/CFInstall.min.js"></script>
    <script>window.attachEvent('onload',function(){CFInstall.check({mode:'overlay'})})</script>
  <![endif]-->
  
</body>
</html>
